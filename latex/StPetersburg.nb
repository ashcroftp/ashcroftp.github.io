Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"computeMat", "[", 
    RowBox[{"log2nStates_", ",", "m_"}], "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"nStates", ",", "weights", ",", "dim", ",", "mat"}], "}"}], ",",
      "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"nStates", "=", 
       RowBox[{"2", "^", "log2nStates"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"weights", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox["p", "n"], 
            RowBox[{"(", 
             RowBox[{"1", "-", "p"}], ")"}]}], ")"}], "/.", 
          RowBox[{"p", "\[Rule]", "0.5"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"n", ",", "0", ",", 
           RowBox[{"Floor", "[", "log2nStates", "]"}]}], "}"}]}], "]"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"dim", "=", 
       RowBox[{"{", 
        RowBox[{"nStates", ",", "nStates"}], "}"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"mat", "=", 
       RowBox[{"Total", "[", 
        RowBox[{"Map", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"SparseArray", "[", 
            RowBox[{
             RowBox[{"Table", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"{", 
                 RowBox[{"i", ",", 
                  RowBox[{"m", "+", "i", "-", 
                   SuperscriptBox["2", "#"]}]}], "}"}], "\[Rule]", 
                RowBox[{"weights", "[", 
                 RowBox[{"[", 
                  RowBox[{"#", "+", "1"}], "]"}], "]"}]}], ",", 
               RowBox[{"{", 
                RowBox[{"i", ",", 
                 SuperscriptBox["2", "#"], ",", 
                 RowBox[{"Min", "[", 
                  RowBox[{
                   RowBox[{"nStates", "-", "m", "+", 
                    SuperscriptBox["2", "#"]}], ",", "nStates"}], "]"}]}], 
                "}"}]}], "]"}], ",", "dim"}], "]"}], "&"}], ",", 
          RowBox[{"Range", "[", 
           RowBox[{"0", ",", 
            RowBox[{"Floor", "[", "log2nStates", "]"}]}], "]"}]}], "]"}], 
        "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"mat", "+=", 
       RowBox[{"SparseArray", "[", 
        RowBox[{
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"{", 
             RowBox[{"i", ",", "i"}], "}"}], "\[Rule]", "1."}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "1", ",", 
             RowBox[{"m", "-", "1"}]}], "}"}]}], "]"}], ",", "dim"}], "]"}]}],
       ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "mat", "]"}]}]}], "\[IndentingNewLine]", "]"}]}],
   "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{"computeP", "[", 
   RowBox[{
   "log2nStates_", ",", "m_", ",", "s_", ",", "log2nIter_", ",", "mat_"}], 
   "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"nStates", ",", "nIter", ",", "P", ",", "Pfinal"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"nStates", "=", 
      RowBox[{"2", "^", "log2nStates"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"nIter", "=", 
      RowBox[{"2", "^", "log2nIter"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"P", "=", 
      RowBox[{"Normal", "[", 
       RowBox[{"SparseArray", "[", 
        RowBox[{
         RowBox[{"s", "\[Rule]", "1."}], ",", "nStates"}], "]"}], "]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"Pfinal", "=", 
      RowBox[{"Nest", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Dot", "[", 
          RowBox[{"mat", ",", "#"}], "]"}], "&"}], ",", "P", ",", "nIter"}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{"1.", "-", 
       RowBox[{"Sum", "[", 
        RowBox[{
         RowBox[{"Pfinal", "[", 
          RowBox[{"[", "i", "]"}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", 
           RowBox[{"m", "-", "1"}]}], "}"}]}], "]"}]}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]}], "Input",
